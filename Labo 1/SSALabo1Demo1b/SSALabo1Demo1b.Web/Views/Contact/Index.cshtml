@{
    ViewBag.Title = "Index";
}

<h2>Contact</h2>
<form method="post" action="/contact/save">
    <div>
        <label for="title">Title:</label>
        <input type="text" id="title" name="title" />
    </div>
    <div>
        <label for="info">Info:</label>
        <textarea name="info" id="info"></textarea>
    </div>
    <div>
        <input type="submit" value="Save" name="save" />
    </div>
    
</form>
